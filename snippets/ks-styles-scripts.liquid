<!-- Async load non-critical Swiper CSS -->
<link rel="preload" href="{{ 'ks-vendor-swiper.bundle.min.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'ks-vendor-swiper.bundle.min.css' | asset_url }}"></noscript>

<script src="{{ 'ks-vendor-swiper.bundle.min.js' | asset_url }}" defer="defer"></script>

<script>
  window.kondasoft = {
    moneyFormat: '{{ shop.money_format }}',
    times: {
      ago: '{{ 'kondasoft.times.ago' | t }}',
      moments: '{{ 'kondasoft.times.moments' | t }}',
      s: '{{ 'kondasoft.times.s' | t }}',
      second: '{{ 'kondasoft.times.second' | t }}',
      seconds: '{{ 'kondasoft.times.seconds' | t }}',
      m: '{{ 'kondasoft.times.m' | t }}',
      minute: '{{ 'kondasoft.times.minute' | t }}',
      minutes: '{{ 'kondasoft.times.minutes' | t }}',
      h: '{{ 'kondasoft.times.h' | t }}',
      hour: '{{ 'kondasoft.times.hour' | t }}',
      hours: '{{ 'kondasoft.times.hours' | t }}',
      d: '{{ 'kondasoft.times.d' | t }}',
      day: '{{ 'kondasoft.times.day' | t }}',
      days: '{{ 'kondasoft.times.days' | t }}',
      expired: '{{ 'kondasoft.times.expired' | t }}'
    },
    wishlist: {
      add: '{{ 'kondasoft.wishlist.add' | t }}',
      remove: '{{ 'kondasoft.wishlist.remove' | t }}',
    }
  }
</script>

